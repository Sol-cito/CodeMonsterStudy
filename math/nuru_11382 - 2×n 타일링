import java.util.*;

public class Main {

    public static void main(String[] args) {
    
        Scanner sc = new Scanner(System.in);

        String cases = sc.nextLine();
        // int로는 범위 커버 불가능
        long sum = 0;
        
        String[] array = cases.split(" ");
        
        for(int i= 0; i<array.length ; i++){
            sum += Long.parseLong(array[i]);
        }
        
        System.out.println(sum);

    }
}

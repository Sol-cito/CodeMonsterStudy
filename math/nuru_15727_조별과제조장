import java.util.*;

public class Main {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int dist = sc.nextInt();
        int result =0;
        if(dist%5==0){
            result = dist/5;
        }else{
            result = dist/5+1;
        }
        System.out.println(result);
    }
    
}

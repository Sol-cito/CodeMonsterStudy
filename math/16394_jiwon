import java.util.Scanner;

public class Main {
    public static int solution(int N) {
		int answer = N - 1946;

		return answer;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);

		int N = sc.nextInt();

		System.out.println(solution(N));

	}
}
